.button {
    position: relative;
    align-items: stretch;
    justify-content: flex-start;
    align-self: flex-start;
    align-content: stretch;
    flex-direction: row;
    width: 110px;
    height: 110px;
    flex-wrap: nowrap;
    font-size: 23px;
}

.text {
    font-size: 40px;
    -unity-text-align: upper-center;
    background-color: rgba(255, 255, 255, 0);
    flex-direction: column;
    justify-content: center;
    align-self: center;
    align-content: center;
}
